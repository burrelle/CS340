<table class="table table-striped" id="quarterbackStatsTable">
  <thead class="thead-inverse">
    <tr>
      <th>
        First Name
      </th>
      <th>
        Last Name
      </th>
      <th>
        Team
      </th>
      <th>
        Passing Yards
      </th>
      <th>
        Passing Attempts
      </th>
      <th>
        Passes Completed
      </th>
    </thead>
    <tbody>
      {{#each quarterbackStats}}
        <td>
          {{this.firstName}}
        </td>
        <td>
          {{this.lastName}}
        </td>
        <td>
          {{this.mascot}}
        </td>
        <td>
          {{this.passingYards}}
        </td>
        <td>
          {{this.passingAttempts}}
        </td>
        <td>
          {{this.passesCompleted}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
